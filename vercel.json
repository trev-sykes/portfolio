{
    "rewrites": [
        {
            "source": "/projects/:projectName",
            "destination": "/index.html"
        },
        {
            "source": "/blogs/:blogName",
            "destination": "/index.html"
        },
        {
            "source": "/projects",
            "destination": "/index.html"
        },
        {
            "source": "/blogs",
            "destination": "/index.html"
        },
        {
            "source": "/about",
            "destination": "/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}